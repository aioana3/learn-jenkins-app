{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "JenkinsApp-Taskdefinition-Pro",
    "containerDefinitions": [
        {
            "name": "jenkinsapp",
            "image": "nginx:1.27-alpine",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::339712840512:role/ecsTaskExecutionRole"
}